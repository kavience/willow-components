{
  "name": "wc-input-number",
  "version": "0.0.1",
  "description": "input number control of react component",
  "author": "kavience <kavience@gmail.com>",
  "main": "lib/index.js",
  "scripts": {
    "dev": "npm run update-demo && dumi dev",
    "build": "npm run update-demo && dumi build && npm run lib:build",
    "lib:build": "cross-env NODE_ENV=prod rollup -c",
    "update-demo": "node update-demo.js",
    "lint": "lint-staged",
    "test": "cross-env IS_TEST=true jest",
    "test:with-coverage": "cross-env IS_TEST=true TEST_COVERAGE=true jest",
    "husky:init": "husky install && husky set .husky/pre-commit 'npm run test' && husky set .husky/pre-commit 'npm run lint'"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "bignumber.js": "^9.0.1",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.16",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rc-util": "^5.12.2",
    "rollup": "^2.47.0",
    "test": "^0.6.0",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kavience/willow-component.git"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,md}": [
      "eslint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-jest": "^26.6.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "ts-jest": "^26.5.6"
  }
}
